<script>
  var batchno = 1;
  var id="";

$(document).ready(function(){
  $("#nextlink").click(function(){
  id = "#batch"+batchno;
  $(id).css("display", "none");
    batchno= batchno+1;
    id = "#batch"+batchno;
    $(id).css("display", "block");
  });

  $("#prevlink").click(function(){
  id = "#batch"+batchno;
  $(id).css("display", "none");
    batchno= batchno-1;
    id = "#batch"+batchno;
    $(id).css("display", "block");
  });
});

</script>


