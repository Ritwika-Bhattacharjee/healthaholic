* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

html,body {
  font-family: "Open Sans", "Trebuchet MS", Helvetica, sans-serif;
  text-align: center;
  min-width: 320px;
  background: linear-gradient(90deg, white, gray);
}

#header {
  display: flex;
  align-items: center;
  position: fixed;
  top: 0;
  width: 100%;
  background: linear-gradient(90deg, white, #7CDEAF);
  justify-content: space-around;
  padding-bottom: 25px;
  padding-top: 25px;
  z-index: 1000;
}

#logo-name {
  display: flex;
  align-items: center;
}

#header-img {
  padding: 7px;
  height: 70px;
  display: flex;
}

#company-name {
  font-size: 2.5em;
  display: flex;
  white-space: nowrap;
  font-weight: bold;
  font-family: 'Merienda One', Helvetica, sans-serif;
  color: rgba(0,0,0,0.7);
  text-shadow: 3px 3px 3px #fff;
}

#nav-bar {
  display: flex;
}

.ul-container {
  padding: 0;
  margin: 0;
  list-style: none;
  width: 30vw;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  font-family: "Arial", "Gadget", sans-serif;
}

.nav-link {
  display: flex;
  font-size: 1.2em;
  text-decoration: none;
}
.ul-container li a{
    color: black;
}

.ul-container li a:hover{
  text-shadow: 3px yellow;
  color: #9BC20D;
  font-weight: bold;
}
.tabbutton{
  width: 100%;
  height: 70px;
  font-family: 'Merienda One', Helvetica, sans-serif;
  /*font-family: 'Raleway', Helvetica, Arial, sans-serif;*/
  /*font-family: 'Just Another Hand', Helvetica, sans-serif;*/
  /*text-shadow: 0px 3px 0px #5e5e5e, 0px 5px 0px #4d4d4d, 0px 7px 0px;*/
  font-size: 20px;
  font-weight: bold;
}

.tabbutton a:link, .tabbutton a:visited, .tabbutton a:hover{
  text-decoration: none;
  color: black;
}
#selected{
  border: 5px solid #A2F94C;
}
#notselected{
  border: 5px solid transparent;
}
#notselected:hover{
  border: 5px solid #ECE811;
}
.tellus{
  width: 100%;
  text-align: left;
  padding-left: 60px;
  font-family: 'Merienda One', Helvetica, sans-serif;
  font-size: 40px;
  padding-top: 30px;
  color: #355818;

}
.symptoms{
  width: 100%;
  text-align: left;
  padding-left: 80px;
  padding-top: 20px;
  font-weight: bold;
  font-size: 20px;
  font-family: "Arial";
}
input+label{
  color: #4C880C;
}
input:checked + label {
  /*color: #80D427;*/
  color: maroon;
  font-size: 30px;
}

#predictbut{
  text-align: center;
  margin-top: 40px;
}
#prebut{
  padding: 10px 60px;
  border-radius: 30px;
  font-family: 'Merienda One', Helvetica, sans-serif;
  font-weight: bold;
  font-size: 20px;
  background: linear-gradient(90deg, #8FC85E, #4C7F21);
  color: white;

}
#prebut:hover{
  border: 5px solid #A9EC33;
}
#batch2, #batch3, #batch4, #batch5{
  display: none;
}
#nextlink, #prevlink{
  border: 5px solid transparent;
  padding-top: 10px;
  padding-bottom: 10px;
  width: 100%;
}
#nextlink:hover, #prevlink:hover{
  border: 5px solid white;
}
#nextlink{
  background-color: #ADEAD6;
}
#prevlink{
  background-color: #E7F67D;
}
@media (max-width: 1024px) {
  #header {
    display: flex;
    flex-flow: row wrap;
  }
  
  #logo-name {
    display: flex;
  }
  
  #header-img {
    height: 53px;
    flex: 1 auto;
  }
  
  #company-name {
    flex: 1 auto;
    font-size: 1.4em;
  }
  
  #nav-bar {
    flex: 1 0 100%;
    margin-top: -10px;
    justify-content: center;
  }
  
  .ul-container {
    margin: 5px;
    flex-direction: column;
    align-items: center;
  }
  
  .nav-link {
    margin: auto;
    font-size: 0.9em;
  }
}

